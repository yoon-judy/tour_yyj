<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="edu.ict.ex.mapper.UserBoardMapper">
    <select id="getUserBoardList" resultType="edu.ict.ex.vo.UserBoardVO">
        SELECT * FROM infotour
    </select>

    <select id="getUserBoardDetail" parameterType="String" resultType="edu.ict.ex.vo.UserBoardVO">
        SELECT * FROM infotour WHERE tour_code = #{tour_code}
    </select>
</mapper>
